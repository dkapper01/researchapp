extends layout

block content
  //h5=title
  .table-responsive
    table.table.table-striped.table-hover.table-bordered.table-margin
      thead
        //tr
        th #
        th Titan
        th Created At
      tbody
        - var count = 0
        each titan in titan_lists
          tr
            td(class='count_width') #{++count}
            td(class='main_width')
              a(href=titan.url) #{titan.titan_name}
            td #{titan.createdAt}

  nav(aria-label='Page navigation')
    ul.pagination
      li
        a(href='#', aria-label='Previous')
          span(aria-hidden='true') «
      li
        a(href='#') 1
      li
        a(href='#') 2
      li
        a(href='#') 3
      li
        a(href='#') 4
      li
        a(href='#') 5
      li
        a(href='#', aria-label='Next')
          span(aria-hidden='true') »